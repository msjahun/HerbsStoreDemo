@model List<HerbsStore.Libraries.HS.Services.FeedbackServices.FeedbackCrudVm>
@{
    ViewData["Title"] = "Feedbacks";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <div class="container">

        <h2 class="typoh2">List of Feed backs</h2>
        <br />

        <div class="bs-docs-example">
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th>#Id</th>
                        <th>Customer Name</th>
                        <th>Customer Email</th>
                        <th>Subject</th>
                        <th>Message</th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                
                @foreach (var item in Model)
                {

             
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.Name </td>
                        <td>@item.Message </td>
                        <td>@item.Subject</td>
                        <td>@item.Message</td>
                        <td>

                            <a asp-controller="Administration" asp-action="DeleteFeedback" asp-route-id="@item.Id" class="btn btn-danger">Delete</a>
                        </td>
                    </tr>

                }
                </tbody>
            </table>
        </div>
        <hr class="bs-docs-separator">
    </div>